{
  "name": "@traitors-ai/shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for Traitors AI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./socket-events": {
      "types": "./dist/socket-events.d.ts",
      "default": "./dist/socket-events.js"
    },
    "./game-types": {
      "types": "./dist/game-types.d.ts",
      "default": "./dist/game-types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
